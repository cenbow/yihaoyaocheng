<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><!DOCTYPE html><html><head lang="en">    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">    <meta name="app-mobile-web-app-capable" content="yes">    <%@ include file="../config.jsp"%>    <style type="text/css">    body,html,p{margin: 0;padding: 0}    body{height: 100%;background: #F3F3F3;font-family: "Microsoft Yahei";color: #333}    .container{width: 64.28%;margin: 20% auto 0;text-align: center}    .container .p1{font-size: 16px;padding-top: 30px;}    .container .p2{font-size: 12px;padding-top: 30px;}    .container .toBuy,.container .viewOrder{display: block;width: 100%;height: 30px;border-radius: 5px;line-height: 30px;text-align: center;font-size: 14px;text-decoration: none;margin-top:10px;}    .container .toBuy{background: #df4038;color: #fff;margin-top:25px;}    .container .viewOrder{background: #fff;color: #333}        .share{ width: 64.28%;margin: 10% auto;text-align: center;background:#df4138; width:95px; height:95px; line-height:95px;border-radius:50px;     -moz-border-radius:50px; -webkit-border-radius:50px; text-align:center; color:#FFF; font-size:18px}    </style>	<script type="text/javascript">		window.onload=function(){ 			window.payresult.disableBackBtn();		}			function backToOrderClick(){			window.payresult.backToOrder();		}		function backToMainClick(){			window.payresult.backToMain();		}		function backToShare(){			window.payresult.backToShare();		}        window.location.href = 'http://m.yaoex.com/pay_result.html';	</script></head>    <body>         <div class="container">            <img src="${ctx}/static/images/order_submit_success.png" width="33.333%" height="auto">            <p class="p1">恭喜您，订单支付成功</p>            <p class="p2">您的订单将根据不同供应商拆成多张，详情请进入订单中心查看</p>            <a onclick='backToMainClick()' class="toBuy">继续购买</a>            <a onclick='backToOrderClick()' class="viewOrder">查看订单</a>         </div>	         <div class="share" onclick="backToShare()">分享有奖</div>    </body></html>